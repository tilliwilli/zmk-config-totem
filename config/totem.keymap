ZMK_COMBO(star,  &kp STAR,      RT2 RM2,     COMBO_LAYERS, COMBO_TERM_SLOW)
ZMK_COMBO(amps,  AMPSHOLD,      RT3 RM3,     COMBO_LAYERS, COMBO_TERM_SLOW)

ZMK_COMBO(under, &kp UNDERSCORE, RM0 RB0,    COMBO_LAYERS, COMBO_TERM_SLOW)
ZMK_COMBO(minus, &kp MINUS,     RM1 RB1,     COMBO_LAYERS, COMBO_TERM_SLOW)
ZMK_COMBO(fslh,  &kp FSLH,      RM2 RB2,     COMBO_LAYERS, COMBO_TERM_SLOW)
