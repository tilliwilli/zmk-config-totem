ZMK_COMBO(gt,    &kp GT,        RB2 RB3,     COMBO_LAYERS, COMBO_TERM_FAST)

/*********************************************************/
// Vertical combos

// Left hand
ZMK_COMBO(at,    &kp AT,        LT3 LM3,     COMBO_LAYERS, COMBO_TERM_SLOW)
